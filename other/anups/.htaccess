RewriteEngine On

DirectoryIndex default.php

RewriteRule styles/([^/]+)$ styles/$1 [NC,L]
RewriteRule styles/([^/]+)/([^/]+)$ styles/$1/$2 [NC,L]

RewriteRule fonts/([^/]+)$ fonts/$1 [NC,L]

RewriteRule js/([^/]+)$ js/$1 [NC,L]
RewriteRule js/([^/]+)/([^/]+)$ js/$1/$2 [NC,L]

RewriteRule contact-us contact.php [NC,L]
RewriteRule about-us about.php [NC,L]
RewriteRule services services.php [NC,L]
RewriteRule crew crew.php [NC,L]
